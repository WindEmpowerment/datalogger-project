#ifndef RTCATMEGA1284P_H_
#define RTCATMEGA1284P_H_

class RTC_atmega1284p {
public:
	RTC_atmega1284p();
	virtual ~RTC_atmega1284p();

	void init();

private:
	unsigned char second;
	unsigned char minute;
	unsigned char hour;
	unsigned char date;
	unsigned char month;
	unsigned char year;
	double timestamp;

};

#endif /* RTCATMEGA1284P_H_ */
